<?xml version='1.0' encoding='utf-8'?>
<widget id="com.looPlayerWeb.looigi" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>looPlayerWebC</name>
    <description>
        LooWebPlayer II
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Looigi
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="loadUrlTimeoutValue" value="700000" />
    <preference name="AndroidExtraFilesystems" value="files,cache, sdcard, cache-external, files-external" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
</widget>
